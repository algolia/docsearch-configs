{
  "index_name": "angular-meteor",
  "start_urls": [
    {
      "url": "https://www.angular-meteor.com/tutorials/socially/(?P<angular_version>.*?)/",
      "variables": {
        "angular_version": [
          "angular1",
          "angular2"
        ]
      }
    },
    {
      "url": "https://www.angular-meteor.com/api/(?P<api_version>.*?)/",
      "variables": {
        "api_version": {
          "url": "https://www.angular-meteor.com/api/1.3.6/helpers",
          "js": "var versions = $('#api-selector option').map(function (i, elt) { return $(elt).html(); }).toArray(); return JSON.stringify(versions);"
        }
      }
    },
    "https://www.angular-meteor.com/api/angular2/",
    {
      "url": "https://www.angular-meteor.com/api/(?P<angular_version>.*?)/(?P<api_version>.*?)/",
      "variables": {
        "api_version": {
          "url": "https://www.angular-meteor.com/api/angular2/0.4.2/meteorComponent",
          "js": "var versions = $('#api-selector option').map(function (i, elt) { return $(elt).html(); }).toArray(); return JSON.stringify(versions);"
        },
        "angular_version": [
          "angular2"
        ]
      }
    }
  ],
  "selectors": {
    "lvl0": ".main-content h1",
    "lvl1": ".main-content h2",
    "lvl2": ".main-content h3",
    "lvl3": ".main-content h4",
    "lvl4": ".main-content h5",
    "lvl5": ".main-content h6",
    "text": ".main-content p"
  },
  "js_render": true,
  "js_wait": 2,
  "docker_cpu": 1,
  "docker_memory": 4096,
  "nb_hits": 2500
}
