{
  "index_name": "vuejs_cn3",
  "start_urls": [
    "https://v3.cn.vuejs.org/"
  ],
  "stop_urls": [
    "^((?!\\.html).)*$"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".depth-0 > .sidebar-heading.open > span, aside > .sidebar-links > li > .active",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".content__default h1",
    "lvl2": ".content__default h2",
    "lvl3": ".content__default h3",
    "lvl4": ".content__default h4",
    "lvl5": ".content__default h5",
    "lvl6": ".content__default h6",
    "text": ".content__default p, .content__default li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "strip_chars": " .,;:#",
  "selectors_exclude": [
    "h1 + blockquote"
  ],
  "scrape_start_urls": false,
  "custom_settings": {
    "attributesForFaceting": [
      "lang"
    ]
  },
  "conversation_id": [
