{
	"index_name": "contember",
	"start_urls": [
	  "https://docs.contember.com"
	],
	"sitemap_urls": [
	  "https://docs.contember.com/sitemap.xml"
	],
	"stop_urls": [
	  "/tests"
	],
	"selectors": {
	  "lvl0": {
		"selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text())[last()]",
		"type": "xpath",
		"global": true,
		"default_value": "Documentation"
	  },
	  "lvl1": "header h1",
	  "lvl2": "article h2",
	  "lvl3": "article h3",
	  "lvl4": "article h4",
	  "lvl5": "article h5, article td:first-child",
	  "lvl6": "article h6",
	  "text": "article p, article li, article td:last-child"
	},
	"custom_settings": {
	  "separatorsToIndex": "_",
	  "attributesForFaceting": [
		"language",
		"version",
		"type",
		"docusaurus_tag"
	  ],
	  "attributesToRetrieve": [
		"hierarchy",
		"content",
		"anchor",
		"url",
		"url_without_anchor",
		"type"
	  ]
	}
  }
