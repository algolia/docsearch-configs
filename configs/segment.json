{
  "index_name": "segment",
  "start_urls": [
    "https://segment.com/docs/"
  ],
  "stop_urls": [
    "/docs/legal/"
  ],
  "sitemap_urls": [
    "https://segment.com/sitemap.xml"
  ],
  "selectors": {
    "lvl0": {
      "selector": "article > .breadcrumbs > li:nth-child(3) > a",
      "global": true,
      "default_value": "Guides"
    },
    "lvl1": {
      "selector": "article > .breadcrumbs > li:nth-child(5) > a",
      "global": true,
      "default_value": "Introduction"
    },
    "lvl2": ".page__body h1",
    "lvl3": ".page__body h2",
    "lvl4": ".page__body h3",
    "lvl5": ".page__body h4, .page__body tr:not(:first-child) td:first-child",
    "lvl6": ".page__body h5",
    "text": ".page__body p, .page__body li, .page__body tr:not(:first-child) td:last-child"
  },
  "stop_content": [
    "This is currently in beta"
  ],
  "min_indexed_level": 3,
  "custom_settings": {
    "separatorsToIndex": "_>@"
  }
}