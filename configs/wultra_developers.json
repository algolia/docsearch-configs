{
  "index_name": "wultra_developers",
  "start_urls": [
    "https://developers.wultra.com/"
  ],
  "sitemap_urls": [
    "https://developers.wultra.com/sitemap.xml"
  ],
  "stop_urls": [],
  "selectors": {
    "lvl0": {
      "selector": "#docsearch-product",
      "global": true
    },
    "lvl1": {
      "selector": "#docsearch-component",
      "global": true
    },
    "lvl2": "section h1",
    "lvl3": "section h2",
    "lvl4": "section h3",
    "lvl5": "section h4",
    "text": "section p, section li"
  },
  "selectors_exclude": [
    ".sidebar-toc",
    ".sidebar"
  ],
  "custom_settings": {
    "attributesForFaceting": [
      "language",
      "version",
      "product",
      "component"
    ],
    "attributesToRetrieve": [
      "language",
      "version",
      "title",
      "product",
      "component",
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
