{
  "index_name": "sendbird",
  "start_urls": [
    {
      "url": "https://docs.sendbird.com/(?P<platform>.*?)/",
      "variables": {
        "platform": [
          "ios",
          "android",
          "javascript",
          "dotnet",
          "unity",
          "platform"
        ]
      }
    }
  ],
  "sitemap_urls": [
    "https://docs.sendbird.com/sitemap.xml"
  ],
  "stop_urls": [
    "change_log$"
  ],
  "selectors": {
    "lvl0": {
      "selector": ".sbdNavDocsItem.isActive",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": "#sbd_contents h2",
    "lvl2": "#sbd_contents h3",
    "lvl3": "#sbd_contents h4",
    "lvl4": "#sbd_contents h5",
    "lvl5": "#sbd_contents h6",
    "text": "#sbd_contents p, #sbd_contents li, #sbd_contents a, #sbd_contents blockquote, #sbd_contents table td p"
  },
  "selectors_exclude": [
    "#change_log"
  ],
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": [
      "version",
      "platform"
    ]
  },
  "only_content_level": true,
  "scrap_start_urls": false,
  "js_render": true,
  "nb_hits": 12003
}